{
  "name": "xbyter/apollo-client",
  "description": "基于携程Apollo 阿波罗的PHP客户端，不依赖第三方扩展和框架并支持自定义配置格式同步（默认支持.env格式配置同步）。",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "Apollo",
    "阿波罗",
    "Client",
    "PHP"
  ],
  "homepage": "https://github.com/xbyter/apollo-client-php",
  "require": {
    "php": "^7.0 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Xbyter\\ApolloClient\\": "src/"
    }
  },
  "require-dev": {
    "vlucas/phpdotenv": "^3.3",
    "phpunit/phpunit": "^6.5.6 || ^7.0 || ^9.6"
  }
}
